{
    "manifest_version": 2,
    "name": "PAUSD Zoom Class Tracker",
    "description": "Track the PAUSD schedule and save zoom links.",
    "version": "1.0",
    "browser_specific_settings": {
        "gecko": {
            "id": "pausdzoomextension@aaryan.dev"
        }
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage"
    ]
}